{
  "name": "scottboyle",
  "version": "2.0.0",
  "private": true,
  "packageManager": "pnpm@10.29.3",
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-object-rest-spread": "^7.28.4",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/register": "^7.28.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "16.3.2",
    "@babel/eslint-parser": "^7.28.6",
    "@eslint/js": "^9.37.0",
    "babel-jest": "^30.2.0",
    "babel-loader": "^8.4.1",
    "caniuse-lite": "^1.0.30000697",
    "core-js": "^2.5.5",
    "coveralls": "^3.0.0",
    "css-loader": "^7.1.4",
    "css-minimizer-webpack-plugin": "^7.0.4",
    "css-variables-loader": "^2.0.2",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "husky": "^0.14.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "marked": "^4.0.10",
    "mini-css-extract-plugin": "^2.10.0",
    "netlify-cli": "^17.0.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.2.1",
    "postcss-preset-env": "^10.6.1",
    "prettier": "^3.6.2",
    "prop-types": "^15.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "timeago-react": "^2.0.0",
    "webpack": "^5.105.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.3"
  },
  "engines": {
    "node": "24.13.1",
    "pnpm": ">=10.29.3"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/scripts/(data|entry).js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\_redirects|.(ico|jpg|png|svg|txt|webmanifest|woff|xml)$": "<rootDir>/test/__mocks__/file.mock.js",
      "\\.(css)$": "<rootDir>/test/__mocks__/style.mock.js"
    },
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.js$": [
        "babel-jest",
        {
          "babelrc": false,
          "configFile": false,
          "plugins": [
            "@babel/plugin-transform-object-rest-spread"
          ],
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ]
    }
  },
  "scripts": {
    "build": "webpack --mode production --progress",
    "dev": "netlify dev --dir build",
    "dev:fast": "webpack serve --mode development --progress",
    "format": "prettier --write \"src/**/*.{css,js}\"",
    "lint": "eslint \"src/**/*.js\" \"netlify/**/*.js\" \"webpack.config.js\"",
    "postbuild": "node src/scripts/sitemap.js && node src/scripts/template.js",
    "predev": "pnpm build",
    "prepush": "pnpm lint && pnpm test:coverage && cat coverage/lcov.info | coveralls",
    "test:coverage": "jest --coverage",
    "test": "jest"
  },
  "dependencies": {
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.30.3"
  }
}
