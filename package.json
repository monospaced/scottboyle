{
  "name": "scottboyle",
  "version": "2.0.0",
  "private": true,
  "packageManager": "pnpm@10.29.3",
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-object-rest-spread": "^7.28.4",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/register": "^7.28.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "16.3.2",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^30.2.0",
    "babel-loader": "^8.4.1",
    "caniuse-lite": "^1.0.30000697",
    "core-js": "^2.5.5",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "css-variables-loader": "^2.0.2",
    "eslint": "^4.19.1",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "marked": "^4.0.10",
    "mini-css-extract-plugin": "^0.5.0",
    "netlify-cli": "^17.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^3.2.1",
    "react-router-to-array": "^0.1.3",
    "react-test-renderer": "^18.2.0",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "timeago-react": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "engines": {
    "node": "24.13.1",
    "pnpm": ">=10.29.3"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/scripts/(data|entry).js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\_redirects|.(ico|jpg|png|svg|txt|webmanifest|woff|xml)$": "<rootDir>/test/__mocks__/file.mock.js",
      "\\.(css)$": "<rootDir>/test/__mocks__/style.mock.js"
    },
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.js$": [
        "babel-jest",
        {
          "babelrc": false,
          "configFile": false,
          "plugins": [
            "@babel/plugin-transform-object-rest-spread"
          ],
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ]
    }
  },
  "scripts": {
    "build": "webpack -p --progress",
    "dev": "netlify dev --dir build",
    "dev:fast": "webpack-dev-server --progress",
    "format": "prettier --write 'src/**/*.@(css|js)'",
    "lint": "eslint \"src/**/*.js\" \"netlify/**/*.js\" \"webpack.config.js\"",
    "postbuild": "node src/scripts/sitemap.js && node src/scripts/template.js",
    "predev": "pnpm build",
    "prepush": "pnpm lint && pnpm test:coverage && cat coverage/lcov.info | coveralls",
    "test:coverage": "jest --coverage",
    "test": "jest"
  },
  "dependencies": {
    "react-helmet-async": "^1.3.0"
  }
}
